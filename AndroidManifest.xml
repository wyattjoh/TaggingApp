<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.ualberta.cs.taggingapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ex_pic3"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ca.ualberta.cs.taggingapp.views.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ca.ualberta.cs.taggingapp.views.PhotoGrid"
            android:label="@string/title_activity_photo_grid" >
        </activity>
        <activity
            android:name="ca.ualberta.cs.taggingapp.views.SignUp"
            android:label="@string/title_activity_sign_up" >
        </activity>
        <activity
            android:name="ca.ualberta.cs.taggingapp.views.SwipeSuper"
            android:label="@string/title_activity_swipe_super" >
        </activity>
    </application>

</manifest>
