<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.taggingapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_action_labels"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:logo="@drawable/ic_action_labels">
        <activity
            android:name="com.example.taggingapp.View.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.taggingapp.View.PhotoGrid"
            android:label="@string/title_activity_photo_grid" >
        </activity>
        <activity
            android:name="com.example.taggingapp.View.SignUp"
            android:label="@string/title_activity_sign_up" >
        </activity>
        <activity
            android:name="com.example.taggingapp.View.SwipeSuper"
            android:label="@string/title_activity_swipe_super" >
        </activity>
        <activity
            android:name="com.example.taggingapp.View.ViewFullPic"
            android:label="@string/title_activity_view_full_pic" >
        </activity>
        <activity
            android:name="com.example.taggingapp.View.TagAndPhoto"
            android:label="@string/title_activity_tag_and_photo" >
        </activity>
        <activity
            android:name="com.example.taggingapp.View.TagRefinedImages"
            android:label="@string/title_activity_tag_refined_images" >
        </activity>
        <activity
            android:name="com.example.taggingapp.View.EditTag"
            android:label="@string/title_activity_edit_tag" >
        </activity>
    </application>

</manifest>
